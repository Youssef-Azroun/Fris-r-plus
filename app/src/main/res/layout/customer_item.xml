<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/custumerAllBookingTextView"
        android:layout_width="385dp"
        android:layout_height="62dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="15dp"
        android:background="@color/transparent_gray"
        android:elegantTextHeight="true"
        android:padding="15dp"
        android:text="custumer all booking"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/deleteTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="5dp"
        android:drawableLeft="@drawable/outline_delete_24"
        app:layout_constraintBottom_toBottomOf="@+id/custumerAllBookingTextView"
        app:layout_constraintEnd_toEndOf="@id/custumerAllBookingTextView" />

</androidx.constraintlayout.widget.ConstraintLayout>